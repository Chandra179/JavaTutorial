<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
		
		
	<servlet>
		<servlet-name>ServletConfigContext</servlet-name>
		<servlet-class>com.practice.ServletConfigContext</servlet-class>
		<init-param>
			<param-name>name</param-name>
			<param-value>Sasuke</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletConfigContext</servlet-name>
		<url-pattern>/servletconfigcontext</url-pattern>
	</servlet-mapping>
	<context-param>
		<param-name>name</param-name>
		<param-value>Xiaomi</param-value>
	</context-param>
	
	
</web-app>